<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

	<changeSet id="organizations-sample-data-0001" author="cursor">
		<loadData
			file="data/organizations.csv"
			relativeToChangelogFile="true"
			tableName="organizations"
			separator="," >
			<column name="id" type="NUMERIC"/>
			<column name="name" type="STRING"/>
			<column name="coordinates_x" type="NUMERIC"/>
			<column name="coordinates_y" type="NUMERIC"/>
			<column name="creation_date" type="DATE"/>
			<column name="annual_turnover" type="NUMERIC"/>
			<column name="full_name" type="STRING"/>
			<column name="employees_count" type="NUMERIC"/>
			<column name="type" type="STRING"/>
			<column name="address_street" type="STRING"/>
			<column name="location_x" type="NUMERIC"/>
			<column name="location_y" type="NUMERIC"/>
			<column name="location_z" type="NUMERIC"/>
			<column name="location_name" type="STRING"/>
		</loadData>
	</changeSet>
</databaseChangeLog> 